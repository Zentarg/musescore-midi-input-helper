{
  "manifest_version": 3,
  "name": "Musescore Midi Input Helper",
  "version": "1.0",
  "description": "A basic Chrome extension with simple JavaScript functionality.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://www.musescore.com/*",
    "https://musescore.com/*"
  ],
  "background": {
    "service_worker": "src/background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.musescore.com/*", "https://musescore.com/*"],
      "js": ["src/content.js"]
    }
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}